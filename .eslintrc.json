{
  "extends": "airbnb-base",
  "plugins": ["import"],
  "consistent-return": 0,
  "rules": {
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "linebreak-style": 0,
    "consistent-return": 0,
    "no-useless-return": 0,
    "no-return-assign": 0,
    "comma-dangle": 0,
    "arrow-body-style": 0,
    "max-len": 0,
    "no-unused-vars": 0,
    "arrow-parens": [2, "as-needed", { "requireForBlockBody": false }],
    "no-useless-constructor": 0,
    "import/no-unresolved": 0
  }
}
